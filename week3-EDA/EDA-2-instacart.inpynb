        "trusted": false
      },
      "cell_type": "code",
      "source": "cnt_srs = order_products_prior_df['product_name'].value_counts().reset_index().head(20)\ncnt_srs.columns = ['product_name', 'frequency_count']\ncnt_srs",
      "execution_count": null,
      "outputs": []
    },
    {
